# KIZI Agent - Environment Variables Template
# Copie este arquivo para .env e preencha com suas chaves

# Groq API (IA gratuita) - https://console.groq.com
VITE_GROQ_API_KEY=gsk_your_key_here

# OpenAI API (opcional) - https://platform.openai.com
VITE_OPENAI_API_KEY=sk-your_key_here

# Anthropic API (opcional) - https://console.anthropic.com
VITE_ANTHROPIC_API_KEY=sk-ant-your_key_here

# Google Gemini API (opcional) - https://aistudio.google.com
VITE_GOOGLE_API_KEY=AIzaSy_your_key_here

# Configurações de segurança
VITE_ENABLE_RATE_LIMIT=true
VITE_MAX_REQUESTS_PER_MINUTE=30
VITE_MAX_MESSAGE_LENGTH=4000

# Configurações de desenvolvimento
VITE_APP_ENV=development
VITE_APP_VERSION=1.0.0

